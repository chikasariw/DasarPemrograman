pajak<-c(7.5, 21.5, 22, 23, 14.5, 16, 19, 20.7, 18.5, 25.9,
23.5, 20, 17, 25.3, 18, 17, 27.1, 28, 24, 30, 22,
20, 20, 31, 31.5)
hist(pajak, xlab = "Pajak Gas ",breaks = 5,
col = "red", border = "black")
hist_data <- hist(pajak, plot = FALSE)
midpoints <- hist_data$mids
counts <- hist_data$counts
# Extend midpoints and counts for polygon
mp_extended <- c(min(midpoints) - diff(midpoints)[1], midpoints, max(midpoints) + diff(midpoints)[1])
counts_extended <- c(0, counts, 0)
plot(mp_extended, counts_extended, type = "l",
xlab = "Value", ylab = "Frequency",
main = "Frequency Polygon of Values",
col = "blue", lwd = 2)
